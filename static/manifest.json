{
	"manifest_version": 3,
	"name": "Binary Tab",
	"description": "Replace the new tab page with a minimal binary clock",
	"version": "2.0.0",

	"permissions": ["storage"],
	"optional_permissions": ["geolocation", "bookmarks"],

	"icons": { "16": "icon-16.png", "48": "icon-48.png", "128": "icon-128.png" },

	"chrome_url_overrides": {
		"newtab": "index.html"
	}
}
